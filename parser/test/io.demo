// test of the if-then and if-then-else logic
procedure main {
  int zero, one, two;
  int three[0:9];

  read three[0];
  zero = 0;
  one = 1;
  three[1] = three[0] + one;
  three[2] = three[1] + one;
  three[3] = three[2] + one;
  two = three[3];
  write two;
  write 0;
  write zero;
  write three[0];
  write three[1];
  write three[2];
  write three[3];
}